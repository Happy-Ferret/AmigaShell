alias endcli="exit"
alias newcli=$SHELL
alias copy="cp"
alias delete="rm"
alias dir="ls -la"
alias info="df"
alias list="ls -la"
alias makedir="mkdir"
alias loadwb="startx"
alias rename="mv"
alias search="find"
alias setclock="date"
alias setdate="date"
alias setmap="loadkeys"
alias stack="ulimit -s"

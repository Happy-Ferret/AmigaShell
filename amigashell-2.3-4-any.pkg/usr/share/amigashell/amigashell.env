alias why="echo return code $?"
alias reboot="zgv -z /usr/share/amigashell/amigashell.png"
alias shutdown="zgv -z /usr/share/amigashell/amigashell.png"

#you might want to add \w (%~ in zsh) to PS1 to see the current working directory
export PS1="`echo $SHLVL`> "
